# Smart Search Sensitive Configuration Variables - EXAMPLE
# Copy this file to .env.secrets and update with your actual credentials
# WARNING: Replace all placeholder values with your actual credentials

# Elasticsearch Passwords
ELASTIC_PASSWORD=your_elasticsearch_password_here
INTERNAL_SEARCH_ENGINE_PASSWORD=your_elasticsearch_password_here

# MongoDB Connection (Internal Container)
MONGODB_URI=mongodb://admin:your_mongo_password@mongodb:27017/ss-test-onestack?authSource=admin&retryWrites=true&w=majority
SPRING_DATA_MONGODB_URI=mongodb://admin:your_mongo_password@mongodb:27017/ss-test-onestack?authSource=admin&retryWrites=true&w=majority

# AWS Configuration (Replace with your actual AWS credentials)
AWS_BUCKET=your-s3-bucket-name
AWS_BUCKET_PREFIX=your-bucket-prefix
AWS_CREDENTIALS_ACCESS_KEY=YOUR_AWS_ACCESS_KEY
AWS_CREDENTIALS_REGION=us-east-2

# JWT Configuration (Timeout only - secrets handled via Docker secrets)
JWT_EXPIRATION_TIME=3600

# License Configuration (Replace with your actual license details)
LICENSE_TYPE=properties
LICENSE_PRODUCT_ID=your-product-id-here
LICENSE_KEY=YOUR-LICENSE-KEY-HERE
LICENSE_OFFLINE_ACTIVATION=false
LICENSE_ON_PREMISE_URL=
LICENSE_USER_ID=your-user-id-here
LICENSE_USER_PLAN=STANDARD_WITH_NLP
LICENSE_USER_PLAN_ID=
LICENSE_USER_DEPLOYMENT_ID=your-deployment-id
LICENSE_CRYPTLEX_SERVICE_URL=https://your-cryptlex-service.com
LICENSE_CRYPTLEX_SERVICE_TOKEN=your-service-token-here

# Application Deployment Configuration
APPLICATION_DEPLOYMENT_ID=your-deployment-id-here

# Reports Engine Password
REPORTS_ENGINE_PASSWORD=your_elasticsearch_password_here
REPORTS_PASSWORD=your_elasticsearch_password_here

# Audit Log Password
AUDIT_LOG_PASSWORD=your_elasticsearch_password_here

# Auth0 Configuration (Replace with your Auth0 credentials)
AUTH0_PUBLIC_KEY=YOUR_AUTH0_PUBLIC_KEY_HERE
REACT_APP_AUTH0_CLIENT_ID=your-auth0-client-id

# Smart Search API Key
SMART_SEARCH_API_KEY=your-api-key-here

# Conversational Search Encryption Key
SMARTSEARCH_ENC_SECRET_KEY=your-encryption-key-here

# Service API Key
SS_SERVICE_API_KEY=your-service-api-key-here
